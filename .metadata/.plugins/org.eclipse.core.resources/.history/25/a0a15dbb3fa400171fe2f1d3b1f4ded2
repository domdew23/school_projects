
import java.util.Arrays;
import java.util.Random;

public class Population {
	int size = 0;
	int pop_size;
	double fitness = 0.0;
	double MUTATION_RATE = 0.01;
	int width = 8;
	int height = 4;
	Member[] members;
	int id;

	Population(int pop_size){
		this.pop_size = pop_size;
		members = new Member[pop_size];
		this.fitness = 0.0;
	}
	
}
